 <div class="section container">
  <div class="row">
    <h4><%= data.attributes.name %></h4>
    <div class="geography__nav">
      <a href="#Single Municipality" class="geography__nav-link">Single Municipality</a>
      <a href="#Multiple Municipalities" class="geography__nav-link">Multiple Municipalities</a>
      <a href="#MAPC Region-Wide" class="geography__nav-link">MAPC Region-Wide</a>
      <a href="#State Wide" class="geography__nav-link">State Wide</a>
    </div>
    <i name="top"></i>
  </div>
  <div class="row">
    <h5><a name="Single Municipality">"Single Municipality" projects assocated with <%= data.attributes.name %></a></h5>
    <% let singleMunicipalityProjects = data.attributes.projects.filter(p => p.project_geography == 'single municipality') %>
    <% let uniqueSingleMunicipalityProjects = _.uniq(singleMunicipalityProjects, 'title') %>
    <% _.each( uniqueSingleMunicipalityProjects, function ( project ) {  %>
    <div><%= projectCardTemplate(project) %></div>
    <% }) %>
  </div>
  <a name="Multiple Municipalities"></a>
  <br />
  <br />
  <br />
  <br />
  <div class="row">
    <p><strong><a href="#top">top^</a></strong></p>
    </p>
    <h5>"Multiple Municipalities" projects assocated with
      <%= data.attributes.name %></h5>
    <% let municipalityWideProjects = data.attributes.projects.filter(p => p.project_geography == 'multiple municipalities') %>
    <% let uniqueMunicipalityWideProjects = _.uniq(municipalityWideProjects, 'title') %>
    <% _.each( uniqueMunicipalityWideProjects, function ( project ) {  %>
    <div><%= projectCardTemplate(project) %></div>
    <% }) %>
  </div>
  <a name="MAPC Region-Wide"></a>
  <br />
  <br />
  <br />
  <br />
  <div class="row">
    <p><strong><a href="#top">top^</a></strong></p>
    </p>
    <h5>"MAPC Region-Wide" projects assocated with <%= data.attributes.name %></h5>
    <% let regionWideProjects = data.attributes.projects.filter(p => p.project_geography == 'mapc region-wide') %>
    <% let uniqueRegionWideProjects = _.uniq(regionWideProjects, 'title') %>
    <% _.each( uniqueRegionWideProjects, function ( project ) {  %>
    <div><%= projectCardTemplate(project) %></div>
    <% }) %>
  </div>
  <a name="State Wide"></a>
  <br />
  <br />
  <br />
  <br />
  <div class="row">
    <p><strong><a href="#top">top^</a></strong></p>
    </p>
    <h5>State Wide" projects assocated with <%= data.attributes.name %></h5>
    <% let stateWideProjects = data.attributes.projects.filter(p => p.project_geography == 'state-wide') %>
    <% let uniqueStateWideProjects = _.uniq(stateWideProjects, 'title') %>
    <% _.each( uniqueStateWideProjects, function ( project ) {  %>
    <div><%= projectCardTemplate(project) %></div>
    <% }) %>
  </div>
</div>
